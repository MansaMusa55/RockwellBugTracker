@model IEnumerable<RockwellBugTracker.Models.Ticket>

<h1> All Tickets</h1>
<div class="card">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Created</th>
                <th>Developer</th>
                <th>Submitter</th>
                <th>Description</th>
                <th>Type</th>
                <th>Priority</th>



            </tr>
        </thead>
        <tbody>
            @foreach (var ticket in Model)
            {
                <tr>
                    <td>@ticket.Title</td>
                    <td>@ticket.Created</td>
                    <td>@ticket.DeveloperUser?.FullName</td>
                    <td>Submitter</td>
                    <td>Description</td>
                    <td>Type</td>
                    <td>Priority</td>
                </tr>
            }
        </tbody>
    </table>
</div>